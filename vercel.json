{
  "version": 2,
  "buildCommand": "next build",
  "routes": [
    { "src": "^/$", "dest": "/api/index.js" },
    { "src": "/login$", "dest": "/api/login.js" },
    { "src": "/login.html", "dest": "/api/login.js" },
    { "src": "/admin/login", "dest": "/api/admin/login.js" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|webp|woff|woff2|ttf|eot))", "dest": "/public/$1" }
  ]
}