{
  "version": 2,
  "buildCommand": "npm install",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/admin/login", "destination": "/server.js" },
    { "source": "/admin/(.*)", "destination": "/server.js" },
    { "source": "/(.*)", "destination": "/server.js" }
  ]
}